double Qapprox_cMOSFET(double Vgs, param_cMOSFET p);
double Ids_cMOSFET(double Vds, double Vgs, param_cMOSFET p);
double Ids_cMOSFET_R(double Vds, double Vgs, param_cMOSFET p);
double Ids0_cMOSFET(double Vds, double Vgs, param_cMOSFET p);
double Ids0_cMOSFET_R(double Vds, double Vgs, param_cMOSFET p);

double Ids_cMESFET(double Vds, double Vgs, param_cMESFET p);
double Ids_cMESFET_R(double Vds, double Vgs, param_cMESFET p);

